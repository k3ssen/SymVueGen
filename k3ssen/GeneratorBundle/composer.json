{
    "name": "k3ssen/generator",
    "description": "Generate content for your symfony5 project",
    "keywords": ["generator", "crud","entity", "entities", "meta", "metadata", "symfony", "symfony5"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Driessen",
            "email": "kevin3ssen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "symfony/yaml": "~5.0",
        "symfony/config": "~5.0",
        "symfony/dependency-injection": "~5.0",
        "symfony/process": "~5.0",
        "symfony/validator": "~5.0",
        "symfony/orm-pack": "~1.0",
        "symfony/console": "~5.0",
        "symfony/expression-language": "~5.0"
    },
    "autoload": {
        "psr-4": { "K3ssen\\GeneratorBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
